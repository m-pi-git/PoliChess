<p></p>
<% @users.each do |user| %>
  <% games = current_user.game_in_progress_with(user) %>
  <% if games.count == 0 %>
    <p></p>
  <% else %>
    <br>You are playing with <%= user.email %> <%= games.count %> games
  <% end %>
  <% games.each do |game| %>
    <p>| <%= link_to "Continue Game", game_path(game) %> |</p>
  <% end %>
<% end %>
